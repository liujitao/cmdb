{
    "go.goroot": "/data/go",
    "go.gopath": "/data/gopath",
    "go.formatTool": "goimports",
    "go.lintFlags": [
        "[\"-min_confidence=.8\"]"
    ],
    "go.vetFlags": [
        "[\"-all\", \"-shadow\"]"
    ],
    "explorer.confirmDelete": false,
    "editor.fontFamily": "'Ubuntu Mono Bold', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 13,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.mouseWheelZoom": false,
    "editor.wordWrap": "on",
    "editor.tabSize": 4,
    "editor.formatOnSave": true
}
